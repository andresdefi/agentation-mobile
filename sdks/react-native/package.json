{
	"name": "@agentation-mobile/react-native-sdk",
	"version": "0.1.0",
	"description": "In-app dev overlay SDK for React Native",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/andresdefi/agentation-mobile.git",
		"directory": "sdks/react-native"
	},
	"keywords": ["agentation", "mobile", "annotation", "mcp", "ai"],
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": ["dist"],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rm -rf dist"
	},
	"dependencies": {
		"@agentation-mobile/core": "workspace:*"
	},
	"peerDependencies": {
		"@react-native-async-storage/async-storage": ">=1.19",
		"react": ">=18",
		"react-native": ">=0.72"
	},
	"devDependencies": {
		"@react-native-async-storage/async-storage": "^2.2.0",
		"@types/react": "^19.0.2",
		"tsup": "^8.3.5"
	}
}
